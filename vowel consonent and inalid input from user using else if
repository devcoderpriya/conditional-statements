#include<stdio.h>
int main()
{
    char a;
    printf("enter the character :");
    scanf("%c",&a);
  if( a=='a'|| a=='e'|| a=='i'|| a=='0'|| a=='u'||
    a=='A'|| a=='E'|| a=='I'|| a=='O'||a=='U')
    {
      printf("vowel");
        
    }
    else if(a>='A'&& a<='z'||a>='A' && a<='Z')
     {
       printf("CONSONENT");
     }
     else
     {
         printf("invalid input");
     }
      return 0;
}
