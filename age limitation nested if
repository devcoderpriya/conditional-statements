#include <stdio.h>


int main() {
  int age;
  // Take present age as input
  scanf("%d", & age);


  if (age > 18) {
    if (age < 25) {
      printf("Ready to start the contest, NINJA! \n");
    } else {
      printf("Not eligible\n");
    }
  } 


else {
    printf("You are not eligible.\n");
  }
  return 0;
}
