#include<stdio.h>
int main()
{
int a;
printf("enter the number:");
scanf("%d",&a);
int b;
printf("enter the number:");
scanf("%d",&b);
int c;
printf("enter the number:");
scanf("%d",&c);
if(a>b){
        if(a>c){
        printf("a is greater");
    }
    else
    printf("c is greater");
}
else
if(b>c){
    printf("b is greater");
}
else
printf("c is greater");
return 0;

}

