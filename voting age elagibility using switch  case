#include<stdio.h>
int main ()
{
  int Age;
  printf ("Tell me your Age:\n");
  scanf ("%d", &Age);
  printf("%d\n",Age);
  switch (Age)
    {
    case (18):
      printf ("You are not elible for voting.\n");
      break;
    case (20):
      printf ("You are eligible for voting.\n");
      break;
    default:
      printf ("No special cases.\n");
      break;
    }
  printf ("Done with switch case.\n");
  return 0;
}
